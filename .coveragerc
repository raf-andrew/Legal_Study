[run]
source = app
omit = 
    */tests/*
    */migrations/*
    */alembic/*
    */venv/*
    */env/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[html]
directory = reports/coverage 