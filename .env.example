APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_LEVEL=debug

# Codespaces Configuration
CODESPACES_ENABLED=true
CODESPACES_GITHUB_TOKEN=
CODESPACES_ORGANIZATION=
CODESPACES_REPOSITORY=
CODESPACES_BRANCH=main
CODESPACES_REGION=us-east-1
CODESPACES_MACHINE=basicLinux32gb
CODESPACES_IDLE_TIMEOUT=30
CODESPACES_RETENTION_PERIOD=1

# Codespaces MySQL Configuration
CODESPACES_MYSQL_HOST=codespaces-mysql
CODESPACES_MYSQL_PORT=3306
