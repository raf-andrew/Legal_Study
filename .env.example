CODESPACES_MYSQL_PASSWORD=

# Codespaces Redis Configuration
CODESPACES_REDIS_HOST=codespaces-redis
CODESPACES_REDIS_PORT=6379
CODESPACES_REDIS_PASSWORD=null

# Codespaces SQLite Configuration
CODESPACES_SQLITE_DATABASE=.codespaces/data/codespaces.sqlite
